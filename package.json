{
  "name": "wickedbased",
  "version": "0.1.0",
  "description": "Type-safe OOP library for DigitalOcean App Platform infrastructure with YAML serialization support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "pnpm run build && pnpm run test"
  },
  "keywords": [
    "digitalocean",
    "app-platform",
    "yaml",
    "infrastructure",
    "pulumi",
    "typescript",
    "validation",
    "zod",
    "devops"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/mimir.git",
    "directory": "packages/wickedbased"
  },
  "bugs": {
    "url": "https://github.com/your-org/mimir/issues"
  },
  "homepage": "https://github.com/your-org/mimir/tree/main/packages/wickedbased",
  "dependencies": {
    "js-yaml": "^4.1.0",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "@pulumi/pulumi": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@pulumi/pulumi": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^3.0.7",
    "fast-check": "^4.5.3",
    "typescript": "^5.7.3",
    "vitest": "^3.0.7"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
